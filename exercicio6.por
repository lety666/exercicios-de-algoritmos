programa{
	funcao inicio(){
	//declaração de variaveis
	real sal,salrec,grat,imp
	//entrada de dados5
     escreva("digite seu salário:")
     leia(sal)
	//processamento de dados
	imp=sal*7/100
	grat=sal*5/100
	salrec=sal-imp+grat
	//saida de dados
	escreva("salário a receber:", salrec)
	}
}
